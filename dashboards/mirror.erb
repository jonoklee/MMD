<% content_for :title do %>Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="clock" data-view="Clock" data-title="Clock" style="background-color:black"></div>
    </li>


    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="welcome" data-view="Text" data-title="Hello" data-text="Good morning Jessica, have a wonderful day!" style="background-color:black"></div>
    </li>
    
	<li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
	    <div data-id="google_calendar" data-view="GoogleCalendar" data-title="Google Calendar" style="background-color:black"></div>
    </li>

	<li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
	    <div data-id="forecast" data-view="Forecast" data-widget_title="Weather Forecast"></div>
	</li>
	
	<li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
	    <div data-id="techcrunch" data-view="News" data-title="TechCrunch" data-interval="10"></div>
    </li>
    
